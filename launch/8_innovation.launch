<launch>

    <!--- Run map tools -->
    <include file="$(find waterplus_map_tools)/launch/wpb_home_nav_test.launch" />
    <!-- Run xfyun IAT -->
    <node pkg="xfyun_waterplus" type="iat_node" name="xfyun_iat_node" output="screen">
        <param name="cn" type="bool" value="true"/>
        <param name="start" type="bool" value="false"/>
    </node>
    <!-- Run xfyun TTS -->
    <node pkg="xfyun_waterplus" type="tts_node" name="xfyun_tts_node" /> 
    <!-- Run sound_play -->
    <node name="soundplay_node" pkg="sound_play" type="soundplay_node.py"/>
    <!-- Run script -->
    <node pkg="wpb_home_apps_2017" type="innovation_test" name="innovation_test" output="screen"/>

</launch>
